@model InvoicePreCheckModels;
<script src="~/js/invoicing.js"></script>
<link href="~/css/invoicing.css" rel="stylesheet" />
@{
    var jsonModel = Newtonsoft.Json.JsonConvert.SerializeObject(Model);
}

<script>
    var model = JSON.parse('@Html.Raw(System.Web.HttpUtility.JavaScriptStringEncode(jsonModel))');
    console.log(model);
</script>
<div class="NetworkToInvoice" id="NetworkToInvoice">
    <h1>Please Select a netwok to check</h1>
    <select>
        <option disabled selected value="">Please Select a option to start</option>
        <option onclick="showSelectedNetworkCheckList(this)" value="Keyfuels">Keyfuels</option>
        <option onclick="showSelectedNetworkCheckList(this)" value="Texaco">Texaco</option>
        <option onclick="showSelectedNetworkCheckList(this)" value="UkFuels">UkFuels</option>
        <option onclick="showSelectedNetworkCheckList(this)" value="FuelGenie">FuelGenie</option>
    </select>
</div>

<div id="CheckListContainer" class="CheckListContainer" hidden>
    <button class="back-button" onclick="goBackFromNetworkCheck(this)">Back</button>
    <table id="CheckListTable" class="CheckListTable">
        <thead>
        </thead>
        <tbody>
        </tbody>
    </table>
    <div class="ButtonContainer">
        <button onclick="ApproveButtonClick()" id="ApproveButton" hidden>Approve</button>
    </div>
</div>

<div hidden id="InvoiceSection" class="InvoiceSection">
    <h1 id="CustNameInvoicing">Customer Name goes here</h1>
    <h3 id="StatusInvoicing">Status:</h3>
    <button onclick="StartInvoicing(this)" id="StartInvoicingBTN">StartInvoicing</button>

</div>